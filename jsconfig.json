{
  "jsx": "react-jsx",
  "exclude": ["node_modules", "**/node_modules/*"],
  "include": ["./src"],
  "compilerOptions": {
    "jsx": "react",
    "moduleResolution": "node",
    "baseUrl": "./src/",
    "paths": {
      "*": ["*"],
      "Components/*": ["components/*"],
      "Containers/*": ["containers/*"],
      "Data/*": ["data/*"],
    },
  },
}
